.header-scroll
	position fixed
	opacity 0
	z-index 3
	visibility hidden
	transform translateY(-100%)
	transition opacity .2s ease, visibility 0s .2s, transform .2s ease
	& .nav
		@media screen and (max-width: 1200px)
			display none
	& .logo
		margin-right 45px
	&.active
		opacity 1
		visibility visible
		transition opacity .2s ease, visibility 0s, transform .2s ease
		transform translateY(0)

		
	
