background_contain(url)
	background: url('../img/'url) center center /contain no-repeat
	
background_cover(url)
	background: url('../img/'url) center center /cover no-repeat
	
body
	min-width 320px

main
	position relative
	font-family 'Roboto'
	
h2
	font-size: 45px
	text-transform uppercase
	font-weight: 900
	text-align center
	letter-spacing: 1px
	@media screen and (max-width: 991px)
			font-size 36px
	@media screen and (max-width: 570px)
			font-size 28px
	@media screen and (max-width: 480px)
			font-size 21px
	
p
	margin 0
	
.container
	padding 0
	@media screen and (max-width: 768px)
			padding 0 15px
	
.btn
	background-color transparent
	padding 0
	border none
	&:focus
		outline none
	&--call
	&--delivery	
		border 1px solid #f08105
		padding: 10px 28px
		border-radius 20px
		color #f08105
		text-transform uppercase
		font-size 12px
		font-weight 700
		transition background .2s ease, color .2s ease
		&:focus
		&:active
		&:hover
			color #f08105
			outline none
	&--delivery
		background-color #f08105
		color #fff
	&--call
		&:hover
			background #f08105
			color #fff
		@media screen and (max-width: 1200px)
			display none
		

